  "description": "This is a repogitory for SSKTS jobs.",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/mocha": "^2.2.43",
    "@types/power-assert": "^1.4.29",
    "@types/sinon": "^2.3.5",
    "concurrently": "^3.5.0",
    "cpx": "^1.5.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.5.5",
    "mocha": "^3.5.3",
    "nsp": "^2.8.1",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.2",
    "sinon": "^3.3.0",
    "snyk": "^1.42.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.3"
  },
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.8"
  },
  "keywords": [],
  "license": "UNLICENSED",
  "name": "sskts-jobs",
  "private": true,
  "repository": {},
  "scripts": {
    "build": "tsc && cpx src/**/*.job dst -v",
    "build:watch": "concurrently \"tsc -w\" \"cpx src/**/*.job dst -v -w\"",
    "check": "npm run tslint && nsp check",
    "clean": "rimraf ./dst test/**/*.js ./doc ./coverage npm-debug.log* ./.nyc_output",
    "doc": "jsdoc -c jsdoc.json",
    "mocha": "mocha src/**/*.spec.ts",
    "snyk": "snyk wizard",
    "test": "npm run check && npm run mocha",
    "tslint": "tslint --type-check --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" src/**/*.ts",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags"
  },
  "version": "3.3.0"
}
