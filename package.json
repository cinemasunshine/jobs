    "@types/power-assert": "^1.5.0",
    "@types/sinon": "^4.3.3",
    "concurrently": "^3.6.1",
    "cpx": "^1.5.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.3",
    "sinon": "^5.1.1",
